{
    "name": "vuevirgin",
    "version": "1.0.1",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "migrate": "knex migrate:latest",
        "rollback": "knex migrate:rollback",
        "refresh": "knex migrate:rollback && knex migrate:latest && knex seed:run",
        "seed": "knex seed:run"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "connect-timeout": "^1.9.0",
        "express": "^4.16.4",
        "express-form-data": "^2.0.6",
        "express-promise-router": "^3.0.3",
        "html-to-text": "*",
        "js-cookie": "^2.2.0",
        "jsonwebtoken": "^8.4.0",
        "knex": "^0.15.2",
        "lodash": "^4.17.11",
        "materialize-css": "^1.0.0",
        "moment": "^2.23.0",
        "multer": "^1.4.1",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "sweetalert": "^2.1.2",
        "vue": "^2.5.17",
        "vue-router": "^3.0.1",
        "vue-socket.io": "^3.0.4",
        "vuetify": "^1.3.0",
        "vuex": "^3.0.1",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.2.0",
        "@vue/cli-plugin-eslint": "^3.2.0",
        "@vue/cli-service": "^3.2.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0-0",
        "faker": "^4.1.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "vue-cli-plugin-vuetify": "^0.4.6",
        "vue-template-compiler": "^2.5.17",
        "vuetify-loader": "^1.0.5"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/pladecompany/vuewebvirgin.git"
    }
}
